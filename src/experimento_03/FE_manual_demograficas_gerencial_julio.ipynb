    },
    {
      "cell_type": "code",
      "source": [
        "generar_interacciones_demograficas <- function(df) {\n",
        "  df <- as.data.table(df)\n",
        "\n",
        "  safe_divide <- function(num, denom) {\n",
        "    result <- ifelse(denom == 0, 0, num / denom)\n",
        "    return(result)\n",
        "  }\n",
        "\n",
        "  df$cliente_joven <- as.integer(df$cliente_edad < 30)\n",
        "\n",
        "  max_edad <- max(df$cliente_edad, na.rm = TRUE)\n",
        "  df$edad_normalizada <- safe_divide(df$cliente_edad, max_edad)\n",
        "\n",
        "  df$cliente_60_mas <- as.integer(df$cliente_edad >= 60)\n",
        "\n",
        "  # Edad x razón saldo-ingreso\n",
        "  df$edad_x_ratio_saldo_ingreso <- df$cliente_edad * df$ratio_saldo_ingreso\n",
        "\n",
        "  # Edad x ingreso ajustado por antigüedad\n",
        "  df$edad_x_ingreso_ajustado <- df$cliente_edad * df$ingreso_ajustado_antiguedad\n",
        "\n",
        "  # Edad x indicador saldo alto\n",
        "  df$edad_x_saldo_alto <- df$cliente_edad * df$saldo_alto\n",
        "\n",
        "  # Edad x productos por antigüedad\n",
        "  df$edad_x_productos_antiguedad <- df$cliente_edad * df$productos_por_antiguedad\n",
        "\n",
        "  # Uso internet x promedio diario consumos\n",
        "  df$internet_x_promedio_diario_consumos <- df$internet * df$promedio_diario_consumos\n",
        "\n",
        "  # Uso internet x ratio comisiones consumos\n",
        "  df$internet_x_ratio_comisiones <- df$internet * df$ratio_comisiones_consumos\n",
        "\n",
        "  # clientes mayores x total movimientos\n",
        "  df$mayores_x_total_movimientos <- df$cliente_60_mas * df$total_movimientos\n",
        "\n",
        "  # clientes mayores x gastos semanales\n",
        "  df$mayores_x_gastos_semanales <- df$cliente_60_mas * df$gastos_semanales\n",
        "\n",
        "  return(df)\n",
        "}\n",
        "\n",
        "# Suponiendo que dataset ya tiene las variables financieras generadas incluyendo gastos_semanales\n",
        "dataset <- generar_interacciones_demograficas(dataset)\n",
        "\n",
        "# Verificamos resultado\n",
        "head(dataset)\n"
      ],
